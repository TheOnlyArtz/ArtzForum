<div class="container">
  <h4 class="center-align grey-text lighten-2">Welcome to <%- subject  %></h4>
  <div class="col s5">
    Here you can view the latest posts.
  </div>
  <div class="row">
    <div class="col s5">
      <% for (let i = 0; i < recentPosts.length; i++) { %>
        <a href="<%- recentPosts[i].postURI %>">
        <div class="card grey darken-2 hoverable">
          <div class="card-content white-text">
              <p class="card-title"><%= recentPosts[i].title %></p>
          </div>
        </div>
      </a>
      <% } %>
    </div>
  </div>
</div>
